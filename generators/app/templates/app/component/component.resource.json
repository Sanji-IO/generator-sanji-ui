{
  "version": "<%= version %>",
  "title": "<%= windowName %>",
  "uuid": "<%= uuid %>",
  "author": {
    "name": "<%= realname %>",
    "email": "<%= email %>"
  },
  "image": "http://placehold.it/220x150",
  "description": "<%= description %>",
  "route": {
    "state": "application.<%= moduleName %>",
    "url": "/<%= moduleName %>"
  },
  "authenticate": ["admin"],
  "files": ["<%= appname %>.js"],
  "get": {
    "url": "<%= endpoint %>",
    "type": "<%= (isCollection) ? 'collection' : 'model' %>"
  },
  "put": {
    "url": "<%= endpoint %><%= (isCollection) ? '/:id' : '' %>",
    "type": "model"
  },
  "fields": []
}

